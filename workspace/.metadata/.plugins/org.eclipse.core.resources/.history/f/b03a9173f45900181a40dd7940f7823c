package com.apps4Society.dao;
import java.sql.*;
public class ConfBanco {
	
	public Connection getConnection(){
		String url = "jdbc:postgresql://localhost:5432/apps4Society";
		Connection com = DriverManager.getConnection(url);
		
		if(com!=null){
			System.out.print("ok");
		}
		return com;
	}

}
